{
  "machine": {
    "gpu": 0,
    "cpu": 1000,
    "ram": 100
  },
  "job": {
    "APIVersion": "V1beta1",
    "Spec": {
      "Deal": {
        "Concurrency": 1
      },
      "Docker": {
        "Entrypoint": [
          "python",
          "/app/test.py",
          "--x",
          {{.X}},
          "--y",
          {{.Y}}
        ],
        "Image": "docker.io/marshath/lilypad-test:v0.0.3"
      },
      "Engine": "Docker",
      "Network": {
        "Type": "None"
      },
      "PublisherSpec": {
        "Type": "Estuary"
      },
      "Resources": {
        "GPU": ""
      },
      "Timeout": 1800,
      "Verifier": "Noop",
      "outputs": [
      {
          "Name": "output",
          "StorageSource": "IPFS",
          "path": "/output"
      }
      ]
    }
  }
}
